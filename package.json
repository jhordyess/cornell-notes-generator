{
  "name": "cornell-notes-generator",
  "version": "1.0.1",
  "description": "A simple Cornell notes generator with React.",
  "scripts": {
    "start": "webpack serve --mode development",
    "start:map": "webpack server --mode development --devtool source-map",
    "build": "webpack --mode production",
    "build:analyze": "webpack --mode production --analyze",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jhordyess/cornell-notes-generator.git"
  },
  "keywords": [
    "react",
    "notes",
    "cornell-notes"
  ],
  "author": "Jhordy Gavinchu <dev@jhordyess.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jhordyess/cornell-notes-generator/issues"
  },
  "homepage": "https://github.com/jhordyess/cornell-notes-generator#readme",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "dependencies": {
    "@react-pdf/renderer": "^2.3.0",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "assert": "^2.0.0",
    "babel-loader": "^8.2.5",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
    "gh-pages": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "path": "^0.12.7",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.3.1",
    "util": "^0.12.4",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  }
}
